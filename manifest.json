{
    "name": "Phoneable",
    "version": "1.0.0",
    "description": "Send Info to Any Mobile Quick!",
    "manifest_version": 3,
    "author": "Kevin Richardson, Samuel Tran",
    "permissions": ["activeTab", "tabs"],
    "action": {
        "default_popup": "index.html",
